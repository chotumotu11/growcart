<h3 class="text-center">Login</h3>
<h4><%= notice %></h4>

<div class="create-user text-center">
	<%= form_for(:user , url: sessions_path) do |f| %>
		<div class="field"><%= f.label :email %> <br/><%= f.text_field :email %></div>
		<div class="filed"><%= f.label :password %> <br/> <%= f.password_field :password %> </div>
		<div class="actions"> <%= f.submit "Login" %></div>
	<% end %>
</div>
<div>
	Not a member ? <%= link_to("Sign Up", new_user_path ) %>
</div>
